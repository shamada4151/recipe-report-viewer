{
  "name": "recipe-report-viewer",
  "version": "0.1.0",
  "description": "Powerful viewer for Automation Center Report",
  "author": "shamada@jeol.co.jp",
  "private": true,
  "main": "./out/main/main.js",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "yarn typecheck:node && yarn typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "yarn typecheck && electron-vite build",
    "build:win": "yarn build && electron-builder --win --config",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.1",
    "@tanstack/react-query": "^4.29.1",
    "@trpc/client": "10.20.0",
    "@trpc/react-query": "10.20.0",
    "@trpc/server": "10.20.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitejs/plugin-react": "^3.1.0",
    "cheerio": "^1.0.0-rc.12",
    "electron": "^24",
    "electron-builder": "^23.6.0",
    "electron-is-dev": "^2.0.0",
    "electron-trpc": "0.5.0",
    "electron-vite": "^1.0.22",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.12.2",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "zod": "^3.21.4"
  }
}
